{
    "extends": [
      "airbnb-base",
      "plugin:@typescript-eslint/recommended"],
    "env": {
        "browser": true
    },
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2015,
        "sourceType": "module"
    },
    "plugins": ["@typescript-eslint/eslint-plugin"],
    "rules": {
      "linebreak-style" : ["error", "windows"],
      "lines-between-class-members" : "off",
      "padded-blocks": "off"
    }
}
